{
  "kind": "collectionType",
  "collectionName": "organizations",
  "info": {
    "name": "Organization"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "website": {
      "type": "string",
      "required": true,
      "regex": "(http(s)?:\\/\\/.)?(www\\.)?[-a-zA-Z0-9@:%._\\+~#=]{2,256}\\.[a-z]{2,6}\\b([-a-zA-Z0-9@:%_\\+.~#?&//=]*)"
    },
    "location": {
      "type": "string",
      "required": true
    }
  }
}
